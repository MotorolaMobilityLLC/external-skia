   "Code": [
      "void spawn(inout Particle p) {",
      "  p.lifetime = 20;",
      "  float a = radians(10 + 60 * rand);",
      "  float s = 140 + rand * 60;",
      "  p.vel.x = cos(a) * s;",
      "  p.vel.y = -sin(a) * s;",
