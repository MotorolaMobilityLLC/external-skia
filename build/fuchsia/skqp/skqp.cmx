{
    "program": {
        "binary": "bin/skqp",
        "args" : [ "/pkg/data", "data/logs" ]
    },
    "sandbox": {
        "dev": [
            "class/display-controller"
        ],
        "features": [
            "vulkan",
            "isolated-persistent-storage"
        ],
        "services": [
            "fuchsia.sysmem.Allocator",
            "fuchsia.tracing.provider.Registry",
            "fuchsia.vulkan.loader.Loader"
        ]
    }
}
